<script type="text/javascript">
var goods_id = 74;
var goodsattr_style = 1;
var gmt_end_time = 0;
var day = "å¤©";
var hour = "å°æ—¶";
var minute = "åˆ†é’Ÿ";
var second = "ç§’";
