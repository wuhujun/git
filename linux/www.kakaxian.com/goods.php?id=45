url = url.replace("url=auto", "url=" + document.URL); 
document.write('<iframe id="WBCommentFrame" src="http://www.kakaxian.com/'&#32;+&#32;url&#32;+&#32;'" scrolling="no" frameborder="0" style="width:770px"></iframe>');
})();
